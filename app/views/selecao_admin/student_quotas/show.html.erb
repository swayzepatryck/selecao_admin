<%- model_class = SelecaoAdmin::StudentQuota  -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:title) %>:</strong></dt>
  <dd id="student_quota_title"><%= @student_quota.title %></dd>
  <dt><strong><%= model_class.human_attribute_name(:abbreviation) %>:</strong></dt>
  <dd id="student_quota_abbreviation"><%= @student_quota.abbreviation %></dd>
</dl>

<div class="form-actions">
  <%= link_to back_icon,
              student_quotas_path, :class => 'btn'  %>
		          <%= link_to edit_icon,
		                      edit_student_quota_path(@student_quota), :class => 'btn', :remote => true %>
  <%= link_to destroy_icon,
              student_quotas_path(@student_quota),
              :method => 'delete',
              :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')),
              :class => 'btn btn-danger' %>
</div>
