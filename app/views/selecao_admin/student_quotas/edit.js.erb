$("#main").before("<%= escape_javascript(render 'edit', :formats => [:html]) %>");
$("#editStudentQuotaModal").modal({
	show: true,
	backdrop: true,
	keyboard: true	
}).on('hidden', function () {
		$('#editStudentQuotaModal').remove();
		$('.processing_action').remove();
		$('input:submit').attr("disabled", false);	 	
});

